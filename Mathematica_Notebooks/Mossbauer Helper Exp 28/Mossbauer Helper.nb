  RowBox[{"ToCurvefitData", "[", "values_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "i", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"n", ",", " ", "xx", ",", " ", "yy", ",", " ", "sy", ",", " ", 
      RowBox[{"sx", " ", "are", " ", "Curvefit", " ", "variables"}]}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "values", "]"}]}], ";", 
     RowBox[{"xx", "=", 
      RowBox[{"{", "}"}]}], ";", 
     RowBox[{"yy", "=", 
      RowBox[{"{", "}"}]}], ";", 
     RowBox[{"sy", "=", 
      RowBox[{"{", "}"}]}], ";", 
     RowBox[{"sx", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "n"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xx", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"xx", ",", 
           RowBox[{"values", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"yy", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"yy", ",", 
           RowBox[{"values", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "2"}], "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"sy", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"sy", ",", 
           RowBox[{"values", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "3"}], "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"sx", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"sx", ",", "0"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True]
